{
  "name": "an-11ty-blog",
  "version": "0.0.2",
  "description": "A blog web site using the Eleventy site generator.",
  "type": "module",
  "scripts": {
    "clean": "rimraf _site",
    "build": "cross-env NODE_ENV=production eleventy",
    "dev": "cross-env NODE_ENV=development npm run clean && eleventy --serve --quiet --incremental"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^6.0.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^2.0.2",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "luxon": "^3.5.0",
    "postcss": "^8.5.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17"
  }
}
